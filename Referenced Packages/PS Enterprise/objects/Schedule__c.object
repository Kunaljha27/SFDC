    <validationRules>
        <fullName>Check_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>End_Date__c &lt;  Start_Date__c</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>End date cannot occur before start date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Hours</fullName>
        <active>true</active>
        <errorConditionFormula>OR(Sunday_Hours__c &lt; 0, Sunday_Hours__c &gt; 24, Monday_Hours__c &lt; 0, Monday_Hours__c &gt; 24, Tuesday_Hours__c &lt; 0, Tuesday_Hours__c &gt; 24, Wednesday_Hours__c &lt; 0, Wednesday_Hours__c &gt; 24, Thursday_Hours__c &lt; 0, Thursday_Hours__c &gt; 24, Friday_Hours__c &lt; 0, Friday_Hours__c &gt; 24, Saturday_Hours__c &lt; 0, Saturday_Hours__c &gt; 24)</errorConditionFormula>
        <errorMessage>Hours cannot be negative or greater than 24.</errorMessage>
    </validationRules>
