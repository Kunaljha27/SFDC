    <fields>
        <fullName>Account__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <deprecated>false</deprecated>
        <description>Unique ID of the account that is associated with the quote.</description>
        <externalId>false</externalId>
        <inlineHelpText>Unique ID of the account that is associated with the quote.</inlineHelpText>
        <label>Account</label>
        <lookupFilter>
            <active>true</active>
            <booleanFilter>(1 AND 2) OR (3 AND 4)</booleanFilter>
            <errorMessage>You should assign only group accounts to group quotes</errorMessage>
            <filterItems>
                <field>$Source.RecordTypeId</field>
                <operation>equals</operation>
                <value>Group Quote</value>
            </filterItems>
            <filterItems>
                <field>Account.MnIsGroupAccount__c</field>
                <operation>equals</operation>
                <value>True</value>
            </filterItems>
            <filterItems>
                <field>$Source.RecordTypeId</field>
                <operation>notEqual</operation>
                <value>Group Quote</value>
            </filterItems>
            <filterItems>
                <field>Account.MnIsGroupAccount__c</field>
                <operation>equals</operation>
                <value>False</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Revvy Quotes</relationshipLabel>
        <relationshipName>Quotes</relationshipName>
        <required>true</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Action__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(RecordType.Name = &apos;Sales Contract&apos;,
  HYPERLINK(&apos;/apex/&apos; + $Setup.MnHierarchicalProperty__c.Revvy_Namespace__c + &apos;PMnQuoteLifeCycleCtrl?action=&apos;+
    IF(ISPICKVAL(Phase__c , &apos;Draft&apos;),&apos;ManageLines&apos;,
      IF(ISPICKVAL(Phase__c , &apos;Authoring&apos;)||ISPICKVAL(Phase__c, &apos;Collaboration&apos;),&apos;Back&apos;,
      &apos;&apos;)
    )
    +&apos;&amp;quoteId=&apos;+ Id ,IMAGE(&apos;/resource/&apos; + $Setup.MnHierarchicalProperty__c.Revvy_Namespace__c +
      IF(ISPICKVAL(Phase__c , &apos;Draft&apos;),&apos;quoteButtonManageLines&apos;,
        IF(ISPICKVAL(Phase__c , &apos;Authoring&apos;)||ISPICKVAL(Phase__c, &apos;Collaboration&apos;),&apos;quoteButtonBack&apos;,&apos;quoteNullimage&apos;)
      ), &apos;&apos;),
    &apos;_self&apos;
  )+
  HYPERLINK(&apos;/apex/&apos; + $Setup.MnHierarchicalProperty__c.Revvy_Namespace__c + &apos;PMnQuoteLifeCycleCtrl?action=&apos;+
    IF(ISPICKVAL(Phase__c, &apos;Draft&apos;)&amp;&amp;!IsSkipDocumentAssembly__c,&apos;AssembleDocument&apos;,
      IF(ISPICKVAL(Phase__c , &apos;Authoring&apos;)||(ISPICKVAL(Phase__c, &apos;Draft&apos;)&amp;&amp;IsSkipDocumentAssembly__c),&apos;SubmitForApproval&apos;,
          IF(ISPICKVAL(Phase__c , &apos;Collaboration&apos;),&apos;SendEmail&apos;,
            IF(ISPICKVAL(Phase__c , &apos;Signature&apos;)&amp;&amp;ISPICKVAL(SubPhase__c, &apos;Completed&apos;),&apos;Activate&apos;,
              IF(ISPICKVAL(Phase__c , &apos;Activation&apos;)&amp;&amp;ISPICKVAL(SubPhase__c, &apos;In Effect&apos;),&apos;Amend&apos;,
              &apos;&apos;)
              )
            )
        )
      )
    +&apos;&amp;quoteId=&apos;+ Id ,IMAGE(&apos;/resource/&apos; + $Setup.MnHierarchicalProperty__c.Revvy_Namespace__c +
    IF(ISPICKVAL(Phase__c, &apos;Draft&apos;)&amp;&amp;!IsSkipDocumentAssembly__c,&apos;quoteButtonAssemble&apos;,
      IF(ISPICKVAL(Phase__c , &apos;Authoring&apos;)||(ISPICKVAL(Phase__c, &apos;Draft&apos;)&amp;&amp;IsSkipDocumentAssembly__c),&apos;quoteButtonSubmitForApproval&apos;,
          IF(ISPICKVAL(Phase__c , &apos;Collaboration&apos;),&apos;quoteButtonSendEmail&apos;,
            IF(ISPICKVAL(Phase__c , &apos;Signature&apos;)&amp;&amp;ISPICKVAL(SubPhase__c, &apos;Completed&apos;),&apos;quoteButtonActivate&apos;,
              IF(ISPICKVAL(Phase__c , &apos;Activation&apos;)&amp;&amp;ISPICKVAL(SubPhase__c, &apos;In Effect&apos;),&apos;quoteButtonAmend&apos;,&apos;quoteNullimage&apos;)
              )
            )
        )
      ), &apos;&apos;),
    &apos;_self&apos;
  )+
  HYPERLINK(&apos;/apex/&apos; + $Setup.MnHierarchicalProperty__c.Revvy_Namespace__c + &apos;PMnQuoteLifeCycleCtrl?action=&apos;+
    IF(ISPICKVAL(Phase__c , &apos;Collaboration&apos;)||(ISPICKVAL(Phase__c , &apos;Signature&apos;)&amp;&amp;ISPICKVAL(SubPhase__c, &apos;In Process&apos;)),&apos;eSignature&apos;,
      &apos;&apos;)
    +&apos;&amp;quoteId=&apos;+ Id ,IMAGE(&apos;/resource/&apos; + $Setup.MnHierarchicalProperty__c.Revvy_Namespace__c +
      IF(ISPICKVAL(Phase__c , &apos;Collaboration&apos;)||(ISPICKVAL(Phase__c , &apos;Signature&apos;)&amp;&amp;ISPICKVAL(SubPhase__c, &apos;In Process&apos;)),&apos;quoteButtonESignature&apos;,
        &apos;quoteNullimage&apos;
      ), &apos;&apos;),
    &apos;_self&apos;
  )+
  IF( !ISPICKVAL(Phase__c, &apos;Termination&apos;) &amp;&amp;
    !(ISPICKVAL(Phase__c, &apos;Activation&apos;) &amp;&amp;
     (ISPICKVAL(SubPhase__c, &apos;In Amendment&apos;) || ISPICKVAL(SubPhase__c, &apos;Superseded&apos;) ) ),
    HYPERLINK(&apos;javascript:if(confirm(&quot;&apos; + $Label.revvy__MnQuoteTerminateConfirmMsg + &apos;&quot;)){window.location.href=&quot;/apex/&apos;+$Setup.MnHierarchicalProperty__c.Revvy_Namespace__c+ &apos;PMnQuoteLifeCycleCtrl?action=Terminate&amp;quoteId=&apos;+ Id+&apos;&quot;}&apos;,
      IMAGE(&apos;/resource/&apos; + $Setup.MnHierarchicalProperty__c.Revvy_Namespace__c + &apos;quoteButtonTerminate&apos;, &apos;&apos;)
      ,&apos;_self&apos;),
    &apos;&apos;
  ),
  &apos;&apos;
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Action</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
