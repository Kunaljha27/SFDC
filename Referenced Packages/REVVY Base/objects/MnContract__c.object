    <fields>
        <fullName>Action__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Status__c , &apos;Active&apos;)||ISPICKVAL(Status__c , &apos;Expired&apos;)||ISPICKVAL(Status__c , &apos;In Renewal&apos;),
  HYPERLINK(&quot;/apex/&quot; + $Setup.MnHierarchicalProperty__c.Revvy_Namespace__c + &quot;PMnContractActionPage?action=renew&amp;contractId=&quot;+ Id , IMAGE(&quot;/resource/&quot; + $Setup.MnHierarchicalProperty__c.Revvy_Namespace__c + &quot;contractRenewButton&quot;,&quot;&quot;),&quot;_self&quot;),
  &apos;&apos;)
  + IF(ISPICKVAL(Status__c , &apos;Active&apos;),
  HYPERLINK(&quot;/apex/&quot; + $Setup.MnHierarchicalProperty__c.Revvy_Namespace__c + &quot;PMnContractActionPage?action=terminate&amp;contractId=&quot;+ Id , IMAGE(&quot;/resource/&quot; + $Setup.MnHierarchicalProperty__c.Revvy_Namespace__c + &quot;contractTerminateButton&quot;,&quot;&quot;),&quot;_self&quot;),
  &apos;&apos;
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Action</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>AdjustmentArrow__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>if ( OverallDiscount__c &gt; 0,
                   IMAGE(&apos;/resource/&apos; + $Setup.MnHierarchicalProperty__c.Revvy_Namespace__c +     &apos;staticui/uifw/ui/images/icons/discount-down.png&apos;, &apos;down&apos; ),
                   if ( OverallDiscount__c &lt; 0,
                     IMAGE(&apos;/resource/&apos; + $Setup.MnHierarchicalProperty__c.Revvy_Namespace__c +     &apos;staticui/uifw/ui/images/icons/discount-up.png&apos;, &apos;up&apos; ),&apos;&apos;
                     )
                 )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>↑↓</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
