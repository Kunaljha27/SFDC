    <validationRules>
        <fullName>VMnPerpetualAndEndDateCheck</fullName>
        <active>true</active>
        <description>Make sure only one of perpetual or end date are set and not both.</description>
        <errorConditionFormula>!(RecordType.DeveloperName == &apos;ServiceContract&apos; || RecordType.DeveloperName == &apos;&apos;) &amp;&amp; Perpetual__c &amp;&amp;  (!ISNULL(EndDateContract__c))</errorConditionFormula>
        <errorMessage>Contracts cannot be Perpetual and have an End date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VMnPerpetualAndEndDateCheck2</fullName>
        <active>true</active>
        <description>make sure one of perpetual or end date exist</description>
        <errorConditionFormula>!(RecordType.DeveloperName == &apos;ServiceContract&apos; || RecordType.DeveloperName == &apos;&apos;) &amp;&amp; !Perpetual__c &amp;&amp;  ISNULL(EndDateContract__c)</errorConditionFormula>
        <errorMessage>End date and Perpetual cannot be both blank. Please set either End date or check Perpetual</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VMnPerpetualAndTermCheck</fullName>
        <active>true</active>
        <errorConditionFormula>!(RecordType.DeveloperName == &apos;ServiceContract&apos; || RecordType.DeveloperName == &apos;&apos;) &amp;&amp; Term__c &gt; 0 &amp;&amp;  Perpetual__c</errorConditionFormula>
        <errorMessage>You cannot mark a Contract Perpetual and provide terms at the same time. Please set terms to blank or uncheck Perpetual.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VMnSegmentDefaultValValidity</fullName>
        <active>true</active>
        <description>Validating Segment to be not null</description>
        <errorConditionFormula>ISPICKVAL(Segment__c,&quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Segment__c</errorDisplayField>
        <errorMessage>Segment cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VmnCurrencyDefaultValValidity</fullName>
        <active>true</active>
        <description>Validating Currency to be not null</description>
        <errorConditionFormula>ISPICKVAL(Currency__c,&quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Currency__c</errorDisplayField>
        <errorMessage>Currency cannot be blank</errorMessage>
    </validationRules>
