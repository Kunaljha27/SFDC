    <validationRules>
        <fullName>VMnCostPriceValidity</fullName>
        <active>true</active>
        <description>Cost Price should be greater than or equal to zero</description>
        <errorConditionFormula>CostPrice__c &lt; 0</errorConditionFormula>
        <errorDisplayField>CostPrice__c</errorDisplayField>
        <errorMessage>Cost Price should be greater than or equal to zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VMnListPriceValidity</fullName>
        <active>true</active>
        <errorConditionFormula>Price__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Price__c</errorDisplayField>
        <errorMessage>List Price should be greater than or equal to zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VMnManDiscountValidity</fullName>
        <active>true</active>
        <description>Manual Discount cannot change when Product is not discountable / line is frozen</description>
        <errorConditionFormula>ISCHANGED(Discount__c) &amp;&amp; ((Frozen__c)  || (!Discountable__c))</errorConditionFormula>
        <errorDisplayField>Discount__c</errorDisplayField>
        <errorMessage>Manual Discount/ Unit Price Cannot be changed if Product is not discountable or line is marked as frozen.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VMnUnitPriceValidity1</fullName>
        <active>true</active>
        <description>Unit Price should be greater than or equal to zero</description>
        <errorConditionFormula>ISPICKVAL(QuoteItem__r.Quote__r.Status__c , &apos;Ready&apos;)  &amp;&amp;  Unit_Price__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Unit_Price__c</errorDisplayField>
        <errorMessage>Unit Price should be greater than or equal to zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VmnQuantityValValidity</fullName>
        <active>true</active>
        <errorConditionFormula>Quantity__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>Quantity__c</errorDisplayField>
        <errorMessage>Quantity cannot be zero or negative</errorMessage>
    </validationRules>
