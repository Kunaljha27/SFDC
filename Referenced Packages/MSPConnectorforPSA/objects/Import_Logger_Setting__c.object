<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Custom setting related to custom logger implemented for Project import process</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Disable_Log__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Field to act as switch to enable and disable logging. Default it will be false i.e log will be record to text file.</description>
        <externalId>false</externalId>
        <inlineHelpText>Field to act as switch to enable and disable logging. Default it will be false i.e log will be record to text file.</inlineHelpText>
        <label>Disable Log</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Log_Level__c</fullName>
        <defaultValue>&quot;ALL&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>Field to support log level for custom logging done for project import process. Support logging levels: ALL &lt; DEBUG &lt; INFO &lt; ERROR. Default as ALL</description>
        <externalId>false</externalId>
        <inlineHelpText>Field to support log level for custom logging done for project import process. Support logging levels: ALL &lt; DEBUG &lt; INFO &lt; ERROR. Default as ALL</inlineHelpText>
        <label>Log Level</label>
        <length>20</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
