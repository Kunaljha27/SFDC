    <fields>
        <fullName>REVVY__Action__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(RecordType.Name = &apos;Sales Contract&apos;,
  HYPERLINK(&apos;/apex/&apos; + $Setup.REVVY__MnHierarchicalProperty__c.REVVY__Revvy_Namespace__c + &apos;PMnQuoteLifeCycleCtrl?action=&apos;+
    IF(ISPICKVAL(REVVY__Phase__c , &apos;Draft&apos;),&apos;ManageLines&apos;,
      IF(ISPICKVAL(REVVY__Phase__c , &apos;Authoring&apos;)||ISPICKVAL(REVVY__Phase__c, &apos;Collaboration&apos;),&apos;Back&apos;,
      &apos;&apos;)
    )
    +&apos;&amp;quoteId=&apos;+ Id ,IMAGE(&apos;/resource/&apos; + $Setup.REVVY__MnHierarchicalProperty__c.REVVY__Revvy_Namespace__c +
      IF(ISPICKVAL(REVVY__Phase__c , &apos;Draft&apos;),&apos;quoteButtonManageLines&apos;,
        IF(ISPICKVAL(REVVY__Phase__c , &apos;Authoring&apos;)||ISPICKVAL(REVVY__Phase__c, &apos;Collaboration&apos;),&apos;quoteButtonBack&apos;,&apos;quoteNullimage&apos;)
      ), &apos;&apos;),
    &apos;_self&apos;
  )+
  HYPERLINK(&apos;/apex/&apos; + $Setup.REVVY__MnHierarchicalProperty__c.REVVY__Revvy_Namespace__c + &apos;PMnQuoteLifeCycleCtrl?action=&apos;+
    IF(ISPICKVAL(REVVY__Phase__c, &apos;Draft&apos;)&amp;&amp;!REVVY__IsSkipDocumentAssembly__c,&apos;AssembleDocument&apos;,
      IF(ISPICKVAL(REVVY__Phase__c , &apos;Authoring&apos;)||(ISPICKVAL(REVVY__Phase__c, &apos;Draft&apos;)&amp;&amp;REVVY__IsSkipDocumentAssembly__c),&apos;SubmitForApproval&apos;,
          IF(ISPICKVAL(REVVY__Phase__c , &apos;Collaboration&apos;),&apos;SendEmail&apos;,
            IF(ISPICKVAL(REVVY__Phase__c , &apos;Signature&apos;)&amp;&amp;ISPICKVAL(REVVY__SubPhase__c, &apos;Completed&apos;),&apos;Activate&apos;,
              IF(ISPICKVAL(REVVY__Phase__c , &apos;Activation&apos;)&amp;&amp;ISPICKVAL(REVVY__SubPhase__c, &apos;In Effect&apos;),&apos;Amend&apos;,
              &apos;&apos;)
              )
            )
        )
      )
    +&apos;&amp;quoteId=&apos;+ Id ,IMAGE(&apos;/resource/&apos; + $Setup.REVVY__MnHierarchicalProperty__c.REVVY__Revvy_Namespace__c +
    IF(ISPICKVAL(REVVY__Phase__c, &apos;Draft&apos;)&amp;&amp;!REVVY__IsSkipDocumentAssembly__c,&apos;quoteButtonAssemble&apos;,
      IF(ISPICKVAL(REVVY__Phase__c , &apos;Authoring&apos;)||(ISPICKVAL(REVVY__Phase__c, &apos;Draft&apos;)&amp;&amp;REVVY__IsSkipDocumentAssembly__c),&apos;quoteButtonSubmitForApproval&apos;,
          IF(ISPICKVAL(REVVY__Phase__c , &apos;Collaboration&apos;),&apos;quoteButtonSendEmail&apos;,
            IF(ISPICKVAL(REVVY__Phase__c , &apos;Signature&apos;)&amp;&amp;ISPICKVAL(REVVY__SubPhase__c, &apos;Completed&apos;),&apos;quoteButtonActivate&apos;,
              IF(ISPICKVAL(REVVY__Phase__c , &apos;Activation&apos;)&amp;&amp;ISPICKVAL(REVVY__SubPhase__c, &apos;In Effect&apos;),&apos;quoteButtonAmend&apos;,&apos;quoteNullimage&apos;)
              )
            )
        )
      ), &apos;&apos;),
    &apos;_self&apos;
  )+
  HYPERLINK(&apos;/apex/&apos; + $Setup.REVVY__MnHierarchicalProperty__c.REVVY__Revvy_Namespace__c + &apos;PMnQuoteLifeCycleCtrl?action=&apos;+
    IF(ISPICKVAL(REVVY__Phase__c , &apos;Collaboration&apos;)||(ISPICKVAL(REVVY__Phase__c , &apos;Signature&apos;)&amp;&amp;ISPICKVAL(REVVY__SubPhase__c, &apos;In Process&apos;)),&apos;eSignature&apos;,
      &apos;&apos;)
    +&apos;&amp;quoteId=&apos;+ Id ,IMAGE(&apos;/resource/&apos; + $Setup.REVVY__MnHierarchicalProperty__c.REVVY__Revvy_Namespace__c +
      IF(ISPICKVAL(REVVY__Phase__c , &apos;Collaboration&apos;)||(ISPICKVAL(REVVY__Phase__c , &apos;Signature&apos;)&amp;&amp;ISPICKVAL(REVVY__SubPhase__c, &apos;In Process&apos;)),&apos;quoteButtonESignature&apos;,
        &apos;quoteNullimage&apos;
      ), &apos;&apos;),
    &apos;_self&apos;
  )+
  IF( !ISPICKVAL(REVVY__Phase__c, &apos;Termination&apos;) &amp;&amp;
    !(ISPICKVAL(REVVY__Phase__c, &apos;Activation&apos;) &amp;&amp;
