      IF(ISPICKVAL(REVVY__Phase__c, &apos;Expiration&apos;), &quot;quoteStageE6Expiration&quot;,
    IF(ISPICKVAL(REVVY__Phase__c , &apos;Draft&apos;)        ,&quot;quoteStage01Draft&quot;,&quot;&quot;)+
    IF(ISPICKVAL(REVVY__Phase__c , &apos;Authoring&apos;)    ,&quot;quoteStage02Authoring&quot;,&quot;&quot;)+
    IF(ISPICKVAL(REVVY__Phase__c , &apos;Approval&apos;)     ,&quot;quoteStage03Approval&quot;,&quot;&quot;)+
